    "TBMJ": {
      "description": "图斑面积字段相关的转换规则",
      "transformations": [
        {
          "name": "标准化面积数据",
          "type": "to_numeric",
          "params": {
            "column": "TBMJ",
            "errors": "coerce"
          },
          "sort_order": 4,
          "enabled": true,
          "description": "确保图斑面积字段为数值类型"
        },
        {
          "name": "四舍五入面积",
          "type": "round_numbers",
          "params": {
            "column": "TBMJ",
            "decimals": 2
          },
          "sort_order": 6,
          "enabled": true,
          "description": "将面积保留2位小数"
        }
      ]
    }
